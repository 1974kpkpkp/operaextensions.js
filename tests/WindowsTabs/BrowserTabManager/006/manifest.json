{
  "name": "tc-BrowserTabManager-006",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Events for creating, moving and closing a tab",
  "background": {
      "page": "background.html"
  },
  "permissions": [
    "tabs", "webRequest", "webRequestBlocking", "contextMenus", "http://*/*","https://*/*"
  ]
}
