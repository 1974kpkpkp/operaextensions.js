{
  "name": "tc-extensions-url-filter-add-002",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adding rules to the allow list, check ID.",
  "background": {
      "page": "background.html"
  },
  "content_scripts": [
      {
        "run_at": "document_start", "matches": ["http://*/*", "https://*/*"],
        "js": ["/operaextensions_injectedscript.js", "/oex/includes/urlfilter.js"]
      }
  ],
  "permissions": [
    "tabs", "http://*/*","https://*/*", "webRequest", "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "/manifest.json"
  ]
}
