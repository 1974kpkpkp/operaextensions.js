{
  "name": "tc-OperaResourceLoader-001",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Relative URL check from the Injected Script Process",
  "background": {
      "page": "background.html"
  },
  "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["/operaextensions_injectedscript.js", "/oex/includes/resourceloadtest.js"]
      }
  ],
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ]
}