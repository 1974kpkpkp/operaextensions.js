{
  "name": "tc-OperaResourceLoader-008",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Relative URL check from the Injected Script Process",
  "background": {
      "page": "background.html"
  },
  "content_scripts": [
      {
        "run_at": "document_start",
        "matches": ["http://*/*", "https://*/*"],
        "js": ["/operaextensions_injectedscript.js", "/oex/includes/resourceloadtest.js"]
      }
  ],
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ],
  "web_accessible_resources": [
    "/oex/resources/file.txt", "/manifest.json"
  ]
}
